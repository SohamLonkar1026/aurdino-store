{
  "name": "auto-parking-qr-system",
  "version": "1.0.0",
  "description": "Automatic parking system with QR scanning, Express backend, SQLite, and React scanner component.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --runInBand",
    "seed": "node scripts/seed.js",
    "dev:web": "vite --config vite.config.js",
    "dev:api": "nodemon server.js",
    "dev:all": "concurrently -n api,web -c blue,green \"npm:dev:api\" \"npm:dev:web\""
  },
  "keywords": [
    "parking",
    "qr",
    "express",
    "sqlite",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.js"
    ]
  },
  "dependencies": {
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "helmet": "^7.1.0",
    "jsqr": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "supertest": "^7.0.0",
    "vite": "^5.4.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "concurrently": "^9.0.1"
  }
}
